{ inputs, rootPath, excludedCrates, ... }:

{
  perSystem =
    { inputs'
    , ...
    }: {
      packages.{{zome_manifest.name}} = inputs.hc-infra.outputs.lib.rustZome {
        inherit excludedCrates;
        workspacePath = rootPath;
        holochain = inputs'.holochain;
        crateCargoToml = ./Cargo.toml;
      };
    };
}

