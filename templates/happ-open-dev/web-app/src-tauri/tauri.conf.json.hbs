{
  "productName": "{{app_name}}",
  "version": "0.0.0",
  "identifier": "{{app_name}}",
  "build": {
    "beforeDevCommand": "pnpm build:happ && cross-env BOOTSTRAP_PORT=$(port) SIGNAL_PORT=$(port) INTERNAL_IP=$(internal-ip --ipv4) concurrently -k \"pnpm local-services\" \"pnpm -F ui start\"",
    "beforeBuildCommand": "pnpm -F ui build && pnpm package:happ",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../ui/dist"
  },
  "app": {
    "withGlobalTauri": false,
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "{{title_case app_name}}",
        "width": 800
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
    ]
  }
}
